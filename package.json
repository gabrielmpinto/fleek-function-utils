{
  "name": "functions-utils",
  "version": "0.1.0",
  "description": "Utilities and types for Fleek Functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "repository": "https://github.com/fleek-platform/functions-utils",
  "homepage": "https://github.com/fleek-platform/functions-utils#readme",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "prettier . --check",
      "eslint",
      "jest --passWithNoTests"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
